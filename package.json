{
	"name": "miniproject",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"start": "set NODE_ENV=production && vite --mode production",
		"prisma:generate1": "npx prisma generate --schema=./prisma/schemaNoSQL.prisma",
		"prisma:generate2": "npx prisma generate --schema=./prisma/schemaSQL.prisma",
		"postinstall": "npx prisma generate --schema=./prisma/schemaNoSQL.prisma && npx prisma generate --schema=./prisma/schemaSQL.prisma"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.0",
		"@iconify/svelte": "^4.0.2",
		"@neoconfetti/svelte": "^2.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"install": "^0.13.0",
		"npm": "^10.9.0",
		"postcss": "^8.4.45",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prisma": "^5.21.1",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.20.0",
		"cloudinary": "^2.5.1",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.4.5",
		"google-auth-library": "^9.14.1",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.8.1",
		"mysql2": "^3.11.0",
		"svelte-loading-spinners": "^0.3.6",
		"uuid": "^10.0.0"
	}
}
